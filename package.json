{
    "name": "react-main",
    "version": "0.1.0",
    "engines": {
        "node": "13.7.0",
        "npm": "6.13.1"
    },
    "homepage": "https://maxim1006.github.io/react-main",
    "private": true,
    "scripts": {
        "client": "cd client && npm start",
        "client:install": "cd client && npm i && cd ../",
        "chat": "cd chat && npm start",
        "server": "nodemon -r esm ./server/server.js --my-custom-var=123",
        "server:prod": "NODE_ENV=production nodemon -r esm ./server/server.js",
        "build": "cd ./client && npm run build && cd ../",
        "start": "node -r esm ./server/server.js",
        "heroku-postbuild": "cd ./client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
        "precommit": "lint-staged",
        "lint": "eslint ./client/src --ext .ts,.tsx,.js,.jsx",
        "lint:fix": "eslint ./client/src --ext .ts,.tsx,.js,.jsx --fix",
        "deploy": "gh-pages -d ./client/build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.2",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-redux": "^7.1.7",
        "@types/react-router-dom": "^5.1.4",
        "@types/styled-components": "^4.4.1",
        "apollo-server": "^2.11.0",
        "apollo-server-express": "^2.11.0",
        "axios": "^0.19.0",
        "body-parser": "^1.18.3",
        "code-prettify": "^0.1.0",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dotenv": "7.0.0",
        "esm": "^3.2.25",
        "express": "^4.16.4",
        "faker": "^4.1.0",
        "fs-extra": "^8.1.0",
        "graphql": "^14.6.0",
        "mongoose": "^5.9.5",
        "nanoid": "^3.1.12",
        "node-fetch": "^2.6.0",
        "raw-body": "^2.4.1",
        "stripe": "6.28.0",
        "typescript": "^3.8.3",
        "gh-pages": "^3.1.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.32.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^5.10.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "stylelint": "^13.13.1",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^22.0.0"
    }
}
