{
    "name": "react-main",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prepare": "cd .. && husky client/.husky",
        "report": "source-map-explorer 'build/static/js/*.js'",
        "report+": "node ./de   v-helpers/webpack-bundle-analyzer.dev-helper.js",
        "report++": "react-app-rewired build --report",
        "moment": "node ./dev-helpers/moment.dev-helper.js",
        "start": "react-app-rewired start",
        "start+": "react-app-rewired start --unused-plugin",
        "build": "react-app-rewired build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "nodemon -r esm ./server/server.js",
        "build:server": "webpack-dev-server --open --entry /webpack-dev-server --output-path /dist --compress --content-base ./build --history-api-fallback",
        "build:serve": "cd ./build && npx http-server index.html --compress --open --static.maxage -1",
        "deploy": "gh-pages -d build",
        "preenvs": "NODE_ENV=prelog node envs-examples.js",
        "envs": "HTTPS=true node envs-examples.js --headed --server --port 5000 -sr",
        "postenvs": "NODE_ENV=postlog node envs-examples.js",
        "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
        "lint:check": "eslint ./src --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "prettier ./ --write",
        "stylelint": "stylelint src/**/*{.css,.less,.scss}",
        "stylelint:fix": "npm run stylelint -- --fix",
        "rm": "rm -rf ./test"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "moduleNameMapper": {
            "^@app(.*)$": "<rootDir>/src$1"
        }
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@floating-ui/react": "^0.27.16",
        "@manaflair/redux-batch": "^1.0.0",
        "@mui/material": "^6.1.9",
        "@reduxjs/toolkit": "2.6.0",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.3.0",
        "@testing-library/user-event": "14.4.1",
        "@types/i18n-js": "^3.8.2",
        "@types/jest": "^28.1.6",
        "@types/lodash": "^4.14.194",
        "@types/node": "^18.6.3",
        "@types/react": "18.0.15",
        "@types/react-dom": "18.0.6",
        "@types/react-redux": "^7.1.24",
        "@types/react-window": "^1.8.5",
        "@types/redux-logger": "^3.0.9",
        "@types/styled-components": "^5.1.25",
        "@types/webpack-env": "^1.17.0",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "axios": "^0.27.2",
        "bootstrap": "^5.2.0",
        "bowser": "^2.11.0",
        "classnames": "2.3.1",
        "dayjs": "1.11.4",
        "firebase": "^9.9.1",
        "history": "^5.3.0",
        "i18n-js": "^4.0.2",
        "i18next": "^21.8.16",
        "i18next-xhr-backend": "^3.2.2",
        "imask": "^6.6.1",
        "install": "^0.13.0",
        "local-web-server": "^5.2.1",
        "lodash": "^4.17.21",
        "mobx": "^6.13.5",
        "mobx-react-lite": "^4.1.0",
        "react": "^18.2.0",
        "react-countup": "^6.3.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^11.18.3",
        "react-intl": "^6.0.5",
        "react-redux": "^8.0.2",
        "react-refresh": "^0.14.0",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "react-stripe-checkout": "^2.6.3",
        "react-transition-group": "^4.4.5",
        "react-window": "^1.8.7",
        "redux": "^4.2.0",
        "redux-devtools-extension": "^2.13.8",
        "redux-persist": "6.0.0",
        "redux-saga": "^1.1.3",
        "redux-thunk": "^2.4.1",
        "reselect": "^4.1.6",
        "styled-components": "^5.3.5",
        "typescript": "5.5.4",
        "unused-webpack-plugin": "^2.4.0",
        "uuid": "^8.3.2",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "devDependencies": {
        "@types/uuid": "^8.3.4",
        "body-parser": "^1.20.0",
        "commander": "^9.4.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-prettier": "5.1.3",
        "esm": "^3.2.25",
        "express": "^4.18.1",
        "faker": "^6.6.6",
        "filemanager-webpack-plugin": "^7.0.0",
        "fs-extra": "^10.1.0",
        "gh-pages": "^4.0.0",
        "husky": "9.0.11",
        "less-loader": "^11.0.0",
        "lint-staged": "^13.0.3",
        "nodemon": "^2.0.19",
        "npm": "^8.15.1",
        "postcss-loader": "^7.0.1",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "react-app-rewire-alias": "^1.1.7",
        "react-app-rewired": "^2.2.1",
        "redux-logger": "^3.0.6",
        "sass": "^1.77.6",
        "sass-loader": "^13.0.2",
        "source-map-explorer": "^2.5.2",
        "stylelint": "^14.9.1",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^26.0.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "4.9.3"
    },
    "standard-version": {
        "types": [
            {
                "type": "feat",
                "section": "Features"
            },
            {
                "type": "fix",
                "section": "Bug Fixes"
            }
        ],
        "issueUrlFormat": "https://my-site/browse/TICKET-{{id}}",
        "issuePrefixes": [
            "TICKET-"
        ],
        "userUrlFormat": "https://git.my.com/{{user}}",
        "compareUrlFormat": "https://git.my.com/{{previousTag}}...{{currentTag}}"
    },
    "overrides": {
        "react-scripts": {
            "typescript": "^5.5.4"
        }
    },
    "homepage": "https://maxim1006.github.io/react-main"
}
