{
    "name": "react-main",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://maxim1006.github.io/react-main",
    "scripts": {
        "report": "source-map-explorer 'build/static/js/*.js'",
        "report+": "node ./dev-helpers/webpack-bundle-analyzer.dev-helper.js",
        "moment": "node ./dev-helpers/moment.dev-helper.js",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "nodemon -r esm ./server/server.js",
        "build:server": "webpack-dev-server --open --entry /webpack-dev-server --output-path /dist --compress --content-base ./build --history-api-fallback",
        "build:serve": "cd ./build && ws --spa index.html --compress --open --static.maxage -1",
        "deploy": "gh-pages -d build",
        "prelog": "NODE_ENV=prelog node ./logs.js",
        "log": "node ./logs.js",
        "postlog": "NODE_ENV=postlog node ./logs.js",
        "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
        "lint:fix": "npm run lint -- --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "npm run lint -- --max-warnings 0"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@manaflair/redux-batch": "^1.0.0",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@reduxjs/toolkit": "^1.4.0",
        "@types/classnames": "^2.2.11",
        "@types/i18n-js": "^3.0.3",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.11.8",
        "@types/react": "^16.9.52",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.6",
        "@types/react-window": "^1.8.2",
        "@types/redux-logger": "^3.0.8",
        "@types/styled-components": "^5.1.4",
        "@types/webpack-env": "^1.15.2",
        "axios": "^0.20.0",
        "bootstrap": "^4.5.0",
        "bowser": "^2.8.1",
        "classnames": "^2.2.6",
        "connected-react-router": "^6.8.0",
        "firebase": "^7.6.0",
        "history": "^4.10.1",
        "i18n-js": "^3.7.1",
        "i18next": "^19.7.0",
        "i18next-xhr-backend": "^3.2.2",
        "install": "^0.13.0",
        "local-web-server": "^4.2.1",
        "lodash": "^4.17.15",
        "react": "^16.11.0",
        "react-countup": "^4.3.3",
        "react-dom": "^16.11.0",
        "react-i18next": "^11.7.2",
        "react-intl": "^5.3.2",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.1",
        "react-stripe-checkout": "^2.6.3",
        "react-transition-group": "^4.3.0",
        "react-window": "^1.8.5",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-form": "^8.2.6",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-components": "^4.4.1",
        "typescript": "^3.8.3",
        "webpack-bundle-analyzer": "^3.8.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "babel-eslint": "10.1.0",
        "babel-plugin-styled-components": "^1.10.6",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-airbnb-typescript": "^7.2.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.2",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "faker": "^4.1.0",
        "fs-extra": "^8.1.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2",
        "node-sass": "^4.13.0",
        "nodemon": "^1.19.4",
        "npm": "^6.13.0",
        "redux-logger": "^3.0.6",
        "source-map-explorer": "^1.8.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "3.11.0",
        "gh-pages": "^3.1.0",
        "pretty-quick": "^2.0.1"
    }
}
