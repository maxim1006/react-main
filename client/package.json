{
    "name": "react-main",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://maxim1006.github.io/react-main",
    "scripts": {
        "prepare": "cd .. && husky install client/.husky",
        "report": "source-map-explorer 'build/static/js/*.js'",
        "report+": "node ./de   v-helpers/webpack-bundle-analyzer.dev-helper.js",
        "report++": "react-app-rewired build --report",
        "moment": "node ./dev-helpers/moment.dev-helper.js",
        "start": "react-app-rewired start",
        "build": "npm run lint && react-app-rewired build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "nodemon -r esm ./server/server.js",
        "build:server": "webpack-dev-server --open --entry /webpack-dev-server --output-path /dist --compress --content-base ./build --history-api-fallback",
        "build:serve": "cd ./build && ws --spa index.html --compress --open --static.maxage -1",
        "deploy": "gh-pages -d build",
        "prelog": "NODE_ENV=prelog node ./logs.js",
        "log": "HTTPS=true node ./logs.js --headed --server --port 5000 -sr",
        "postlog": "NODE_ENV=postlog node ./logs.js",
        "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx --no-ignore",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "prettier ./ --write",
        "stylelint": "stylelint src/**/*{.css,.less,.scss}",
        "stylelint:fix": "npm run stylelint -- --fix",
        "rm": "rm -rf ./test"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "moduleNameMapper": {
            "^@app(.*)$": "<rootDir>/src$1"
        }
    },
    "dependencies": {
        "@manaflair/redux-batch": "^1.0.0",
        "@reduxjs/toolkit": "^1.8.3",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.3.0",
        "@testing-library/user-event": "14.4.1",
        "@types/classnames": "^2.3.0",
        "@types/i18n-js": "^3.8.2",
        "@types/jest": "^28.1.6",
        "@types/lodash": "^4.14.182",
        "@types/node": "^18.6.3",
        "@types/react": "18.0.15",
        "@types/react-dom": "18.0.6",
        "@types/react-redux": "^7.1.24",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-window": "^1.8.5",
        "@types/redux-logger": "^3.0.9",
        "@types/styled-components": "^5.1.25",
        "@types/webpack-env": "^1.17.0",
        "axios": "^0.27.2",
        "bootstrap": "^5.2.0",
        "bowser": "^2.11.0",
        "classnames": "^2.3.1",
        "dayjs": "1.11.4",
        "firebase": "^9.9.1",
        "history": "^5.3.0",
        "i18n-js": "^4.0.2",
        "i18next": "^21.8.16",
        "i18next-xhr-backend": "^3.2.2",
        "install": "^0.13.0",
        "local-web-server": "^5.2.1",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-countup": "^6.3.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^11.18.3",
        "react-intl": "^6.0.5",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "react-stripe-checkout": "^2.6.3",
        "react-transition-group": "^4.4.5",
        "react-window": "^1.8.7",
        "redux": "^4.2.0",
        "redux-devtools-extension": "^2.13.8",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "redux-thunk": "^2.4.1",
        "reselect": "^4.1.6",
        "styled-components": "^5.3.5",
        "typescript": "4.7.4",
        "uuid": "^8.3.2",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "devDependencies": {
        "@types/uuid": "^8.3.4",
        "body-parser": "^1.20.0",
        "commander": "^9.4.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "esm": "^3.2.25",
        "express": "^4.18.1",
        "faker": "^6.6.6",
        "filemanager-webpack-plugin": "^7.0.0",
        "fs-extra": "^10.1.0",
        "gh-pages": "^4.0.0",
        "husky": "^8.0.1",
        "less-loader": "^11.0.0",
        "lint-staged": "^13.0.3",
        "node-sass": "^7.0.1",
        "nodemon": "^2.0.19",
        "npm": "^8.15.1",
        "postcss-loader": "^7.0.1",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "react-app-rewire-alias": "^1.1.7",
        "react-app-rewired": "^2.2.1",
        "redux-logger": "^3.0.6",
        "sass-loader": "^13.0.2",
        "source-map-explorer": "^2.5.2",
        "stylelint": "^14.9.1",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^26.0.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "4.9.3"
    }
}
