        "webpack-cli": "^5.1.4"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "build:server": "webpack-dev-server --open --entry /webpack-dev-server --output-path /dist --compress --content-base ./build --history-api-fallback",
        "test": "react-app-rewired test",
        "eject": "react-app-rewired eject",
        "report": "react-app-rewired build --report",
        "codegen:gql": "graphql-codegen --config codegen.yml",
        "codegen": "apollo client:codegen --localSchemaFile schema.graphql --target typescript --watch !!!second option dont use"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/introspection": "4.0.0",
